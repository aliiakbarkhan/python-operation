    "def main():\n",
    "    ins = input(\"Input: \")\n",
    "    f = input(\"Font: \")\n",
    "    fontss(ins,f)\n",
    "\n",
    "def fontss(a,b):\n",
    "    from pyfiglet import Figlet\n",
    "    figlet = Figlet()\n",
    "    figlet.getFonts()\n",
    "    figlet.setFont(font=b)\n",
    "    print(\"Output:\",figlet.renderText(a))\n",
    "\n",
    "main()\n",
    "    \n"
   ]
